{
  "name": "duckdb-demo-server",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "watch": "nodemon --watch . -e ts --exec npm run dev:start",
    "start": "SERVER_PORT=8080 node .",
    "dev:start": "npm run build && npm start",
    "buildDB": "node scripts/createDB.js",
    "build": "rm -rf dist/* && tsc && cp dbConfig.js dist/",
    "tsc": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "duckdb": "0.3.1",
    "express": "4.17.2",
    "socket.io": "4.4.1"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node": "17.0.8",
    "nodemon": "2.0.15",
    "tslint": "6.1.3",
    "typescript": "4.5.4"
  }
}
